{
    "builds": [
        {
            "src": "web/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "web/$1"
        }
    ],
    "functions": {
        "web/src/pages/api/**/*.ts": {
            "maxDuration": 60
        }
    }
}